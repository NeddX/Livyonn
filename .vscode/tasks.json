{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "g++",
            "args": [
                "-fdiagnostics-color=always",
                "-std=c++20",
                "-o",
                "${workspaceFolder}/bin/Debug/liv",
                "-g",
                "${workspaceFolder}/src/main.cpp",
                "${workspaceFolder}/src/Lib/src/Utils.cpp",
                "${workspaceFolder}/src/Analyzer/Armavir/src/Lexer.cpp",
                "${workspaceFolder}/src/Analyzer/Armavir/src/Analyzer.cpp",
                "${workspaceFolder}/src/Analyzer/Armavir/src/FunctionDefinition.cpp",
                "${workspaceFolder}/src/Analyzer/Armavir/src/Statement.cpp",
                "${workspaceFolder}/src/Compilers/AVM/src/AVM.cpp",
                "${workspaceFolder}/src/Compilers/AVM/src/Runtime.cpp",
                "${workspaceFolder}/src/Compilers/AVM/src/ByteBuffer.cpp",
                "${workspaceFolder}/src/Compilers/AVM/src/Registers.cpp"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}