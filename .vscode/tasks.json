{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "g++",
            "args": [
                "-fdiagnostics-color=always",
                "-std=c++20",
                "-o",
                "${workspaceFolder}/bin/Debug/liv",
                "-g",
                "${workspaceFolder}/src/main.cpp",
                "${workspaceFolder}/src/Lib/src/Utils.cpp",
                "${workspaceFolder}/src/Analyzers/Armavir/src/Lexer.cpp",
                "${workspaceFolder}/src/Analyzers/Armavir/src/Analyzer.cpp",
                "${workspaceFolder}/src/Analyzers/Armavir/src/FunctionDefinition.cpp",
                "${workspaceFolder}/src/Analyzers/Armavir/src/Statement.cpp",
                "${workspaceFolder}/src/Runtime/AVM/src/AVM.cpp",
                "${workspaceFolder}/src/Runtime/AVM/src/Runtime.cpp",
                "${workspaceFolder}/src/Runtime/AVM/src/ByteBuffer.cpp",
                "${workspaceFolder}/src/Runtime/AVM/src/Registers.cpp",
                "${workspaceFolder}/src/Analyzers/AVM/src/Analyzer.cpp",
                "${workspaceFolder}/src/Analyzers/AVM/src/Lexer.cpp"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Test/cspp/cspp.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/Test/cspp/cspp.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/Test/cspp/cspp.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ],
    "version": "2.0.0"
}