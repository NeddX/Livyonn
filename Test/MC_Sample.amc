POP %RAX ; Retrieving the first parameter and pushing it tothe RAX register.
POP %RBX ; Retrieving the second parameter and pushing it to the RBX register.

CALL @l1
RETURN

@l1:
    CIGT %RAX, #0
    CJRP #3
    MOV %FNR, #1
    RETURN

    MOV %RDX, %RBX
    PUSH %RAX
    PINT
    CALL @l2
    SUB %RAX, %FNR
    JMP @l1

@l2:
    CIGT %RDX, #0
    CJRP #3
    MOV %FNR, #1
    RETURN

    ;PUSH %RDX
    ;PINT
    SUB %RDX, #1
    JMP @l2
